#include <stdio.h>
#include <string.h>

int main() {
    char str[55];
    int found = 0;

    for (int i = 1; i <= 5; i++) {
        scanf("%54s", str);

        if (strstr(str, "FBI") != NULL) {
            if (found > 0) {
                printf(" ");
            }
            printf("%d", i);
            found = 1;
        }
    }

    if (!found) {
        printf("HE GOT AWAY!");
    }

    return 0;
}

